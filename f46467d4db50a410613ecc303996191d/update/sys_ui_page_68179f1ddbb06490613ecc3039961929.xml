        <client_script><![CDATA[function updateNow(value) {	
	spin(true);
	
	// connection..
	var srv = document.getElementById("server").value;
	var dba = document.getElementById("database").value;
	var aut = document.getElementById("int_auth").checked.toString();
	var usr = document.getElementById("user").value;
	var pwd = document.getElementById("pwd").value;
	var mid = document.getElementById("mid_server").value;
	
	// general..
	var sta = document.getElementById("syncstatus").checked.toString();
	
	// dashboard..
	var rat = document.getElementById("refresh_rate").value;
	
	// schedule..
	var schedule_active = document.getElementById("activeSchedule").checked;
	var runType = document.getElementById("frequency").value;
		
	// weekly value
	var dayWeek = document.getElementById("week-day");
	if (dayWeek) 
		dayWeek = dayWeek.value;
	else
		dayWeek = 1;
	
	// monthly value
	var dayMonth = document.getElementById("month_day");
	if (dayMonth)
		dayMonth = dayMonth.value;
	else
		dayMonth = 1;
	
	// add seconds to the time 
	var runtime = document.getElementById("time").value + ":00";
	
	// periodically values
	var days = document.getElementById("per-days").value;
	var hours = document.getElementById("per-hours").value;
	var min = document.getElementById("per-min").value;
	var durationDisplay = ''; 
	if (days > 0) {
		var sufD = ((days == 1) ? ' Day' : ' Days');
		durationDisplay = days + sufD;
	}
	if (hours > 0) {
		var sufH = ((hours == 1) ? ' Hour' : ' Hours');		
		durationDisplay = ((durationDisplay == '') ? hours + sufH : durationDisplay + ' ' + hours + sufH);
	}
	if (min > 0) {
		var sufM = ((min == 1) ? ' Minute' : ' Minutes');		
		durationDisplay = ((durationDisplay == '') ? min + sufM : durationDisplay + ' ' + min + sufM);
	}
	
	if (runType == 'periodically' && durationDisplay == '') {
		alert('At least one of the periodic values (days, hours or minutes) must be set');
		spin(false);
		return;
	}
	
	var startOn = document.getElementById("per-start").value;
	startOn = startOn.split('T');
	var period_start = startOn[0] + ' ' + startOn[1] + ':00';
	
	var updObj = {
		'sv': srv,
		'db': dba,
		'au': aut,
		'ur': usr,
        'pw': pwd,
        'mid': mid,
		'rt': rat,
		'sta': sta,
		'schedule': {
			'active': schedule_active,
			'run_type': runType,
			'day_of_week': dayWeek,
			'day_of_month': dayMonth,
			'run_time': runtime,
			'period_days': days,
			'period_hours': hours,
			'period_min': min,
			'duration_display': durationDisplay,
			'period_start': period_start
		}
	};
	//alert("Settings update object: " + JSON.stringify(updObj));
	var ajax = new GlideAjax('LansweeperIntegrationAjax');
	ajax.addParam('sysparm_name', 'updateSettings');
	ajax.addParam('sysparm_obj', JSON.stringify(updObj));
	ajax.getXML(processUpdateResponse);
	
}
function processUpdateResponse(response){	
	var answer =  response.responseXML.documentElement.getAttribute("answer");
	// disable update
	document.getElementById("pwd").value = '';
	var btnUpd = document.getElementById("btn_upd");
	btnUpd.disabled = true;
	btnUpd.style.backgroundColor = "#E8E8E8";	
	spin(false);				
}

function spin(spinActive) {
	var spinner = document.getElementById("spinner");
	if (spinActive) {
		spinner.style.display = '';
	} else {
		spinner.style.display = 'none';
	}
}

function changes() {	
	// enable update
	var btnUpd = document.getElementById("btn_upd");
	btnUpd.disabled = false;
	btnUpd.style.backgroundColor = "#1db8d9";
} ]]></client_script>
