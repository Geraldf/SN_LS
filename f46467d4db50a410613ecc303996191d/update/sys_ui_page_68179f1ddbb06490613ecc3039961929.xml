<script>
	
	document.title = "Pipefish CI Synchronizer for Enterprise - Lansweeper On-Premise Edition";		
	
	$j(document).ready(function() {	
		// initialises choice visibility
		var startFreq = $j("#frequency").val();
		if (startFreq == 'weekly') {
				$j('#week-ctrl').show();
				$j('#time-ctrl').show();
				$j('#month-ctrl').hide();	
				$j('#period-ctrl').hide();
			} else if (startFreq == 'monthly') {
				$j('#week-ctrl').hide();
				$j('#month-ctrl').show();
				$j('#time-ctrl').show();
				$j('#period-ctrl').hide();
			} else if (startFreq == 'periodically') {
				$j('#week-ctrl').hide();
				$j('#month-ctrl').hide();
				$j('#time-ctrl').hide();
				$j('#period-ctrl').show();	
			} else {
				$j('#time-ctrl').show();
				$j('#week-ctrl').hide();
				$j('#month-ctrl').hide();
				$j('#period-ctrl').hide();
			}
	
		// initialise range values (did not work on html)
		$j('#per-min').attr({"max": 59,"min": 0});
	    $j('#per-hours').attr({"max": 23,"min": 0});
        $j('#per-days').attr({"max": 365,"min": 0});
        
        // initialise user/pwd credentials visibility
        var integrated_auth = $j("#int_auth").prop('checked');
        if (integrated_auth) {
            $j('#credentials').hide();
        } else {
            $j('#credentials').show();
        }

        // handle user/pwd credentials visibilit`y
        $j('#int_auth').change(function(event) {
            if (this.checked) {
                $j('#credentials').hide();
            } else {
                $j('#credentials').show();
            }
        });
	
		// handle frequency change 
		$j('#frequency').change(function(event) {
			if (this.value == 'weekly') {
				$j('#week-ctrl').show();
				$j('#time-ctrl').show();
				$j('#month-ctrl').hide();	
				$j('#period-ctrl').hide();
			} else if (this.value == 'monthly') {
				$j('#week-ctrl').hide();
				$j('#month-ctrl').show();
				$j('#time-ctrl').show();
				$j('#period-ctrl').hide();
			} else if (this.value == 'periodically') {
				$j('#week-ctrl').hide();
				$j('#month-ctrl').hide();
				$j('#time-ctrl').hide();
				$j('#period-ctrl').show();	
			} else {
				$j('#time-ctrl').show();
				$j('#week-ctrl').hide();
				$j('#month-ctrl').hide();
				$j('#period-ctrl').hide();
			}
		});	
	});
		
</script>	

