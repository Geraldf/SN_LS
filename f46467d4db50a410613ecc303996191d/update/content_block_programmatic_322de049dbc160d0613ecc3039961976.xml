        <name>Dashboard Application Info</name>
        <programmatic_content><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
<style>
  .wrapper {
	padding-left: 5px;
	padding-right: 5px;
	overflow: hidden;
	}	
</style>
	
<div class="wrapper">
	<div id="appDetails">	
	</div>					
</div>	
<script>
	var ga = new GlideAjax('LansweeperIntegrationAjax');
	ga.addParam('sysparm_name', 'getIntegrationAppInfo');
	ga.getXML(appInfoResponse);

	function appInfoResponse(response) {
		var result = response.responseXML.documentElement.getAttribute("answer");
		var appinfo = JSON.parse(result);
		var appEdition = '';
		var previewlink = '';
	
		var html = [];
		if (appinfo.product != '') 
			html.push('<h4 align="left" style="font-weight: bold;text-align:left;">' + appinfo.product + '</h4>');
	
		if (!appinfo.isFullVersion) {
			appEdition = '<h5 align="left" style="font-weight: bold;">' + appinfo.displayName + '</h5>';
			previewlink = '<a href="' + appinfo.url + '" target="_blank" rel="noopener noreferrer">' + appinfo.urlText + '</a>';
		} else {
			appEdition = '<h4 align="left" style="font-weight: bold;">' + appinfo.displayName + '</h4>';
			previewlink = '';
		}
	
		html.push(appEdition);
		
		if (previewlink != '')
			html.push(previewlink);

		appDetails.innerHTML = html.join("");
	}
</script>	
		
</j:jelly>]]></programmatic_content>
