        <sql_statement>SELECT&#13;
	Base_CorrelationID&#13;
	, Base_Name&#13;
	, relationship&#13;
	, Dependant_CorrelationID&#13;
	, Dependant_Name&#13;
FROM&#13;
	(&#13;
		SELECT &#13;
			CAST(tblSqlServerCluster.Id AS VARCHAR(MAX)) + ':' + CAST(tblSqlServerClusterNOde.Id AS VARCHAR(MAX)) + ':' + COALESCE(RTRIM(LTRIM(tblSqlServerClusterNode.Name)), '') AS Dependant_CorrelationID&#13;
			, CAST(tblSqlServerClusterNode.Name AS VARCHAR(MAX)) AS Dependant_Name &#13;
			, 'Cluster of::Cluster' relationship&#13;
			, CAST(tblSqlServerCluster.Id AS VARCHAR(MAX)) + ':' + COALESCE(RTRIM(LTRIM(tblSqlServerCluster.Name)), '') AS Base_CorrelationID&#13;
			, CAST(tblSqlServerCluster.Name AS VARCHAR(MAX)) AS Base_Name&#13;
		FROM&#13;
			dbo.tblSqlServerCluster&#13;
			INNER JOIN tblSqlServerClusterNOde&#13;
				ON tblSqlServerCluster.Id = tblSqlServerClusterNode.ClusterId&#13;
	) rships&#13;
&#13;
</sql_statement>
