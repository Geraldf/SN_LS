        <sql_statement>SELECT &#13;
	tblSqlServers.sqlServerId&#13;
	, tblASsets.AssetID&#13;
	, 'Lansweeper' AS DiscoverySource&#13;
	, 'Resource' AS Category&#13;
	, 'Database' AS Subcategory&#13;
	, CAST(tblSqlServers.sqlServerId AS VARCHAR) + ':' + CAST(tblSqlDatabases.sqlDatabaseId AS VARCHAR) AS CorrelationID&#13;
	, tblSqlServers.IsClustered AS is_clustered&#13;
	, tblState.Statename AS OperationalStatus&#13;
	, 1 AS InstallStatus&#13;
	, tblSqlDatabases.name AS [Name]&#13;
	, tblSqlDatabases.name AS [short_description]&#13;
	, tblSqlDatabases.name AS [database]&#13;
FROM&#13;
	tblSqlServers&#13;
	INNER JOIN tblSqlDatabases&#13;
		ON tblSqlServers.sqlServerId = tblSqlDatabases.sqlServerId&#13;
	INNER JOIN tblAssets&#13;
		ON tblSqlServers.AssetID = tblAssets.AssetID&#13;
	LEFT OUTER JOIN tblAssetCustom &#13;
		ON tblAssets.AssetID = tblAssetCustom.AssetID&#13;
    LEFT OUTER JOIN tblState on tblState.State = tblAssetCustom.State&#13;
&#13;
</sql_statement>
